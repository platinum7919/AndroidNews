<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent" android:layout_height="match_parent">


    <com.iitr.kaishu.nsidedprogressbar.NSidedProgressBar
            android:id="@+id/nSideProgressBar_loadingview"
            android:layout_width="@dimen/size_loading_progress_bar"
            android:layout_height="@dimen/size_loading_progress_bar"
            app:nsidedProg_primaryColor="@color/primarycolor_progressbar"
            app:nsidedProg_secondaryColor="@color/secondarycolor_progressbar"
            android:layout_gravity="center"
            app:nsidedProg_baseSpeed="5"
            app:nsidedProg_primaryRimWidth="12"
            app:nsidedProg_sideCount="256"
            app:nsidedProg_clockwise="true"/>

</FrameLayout>